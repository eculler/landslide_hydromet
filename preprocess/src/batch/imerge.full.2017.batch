  'conda activate hydrometenv && python /projects/elcu4811/landslide.hydromet/landslide.hydromet.git/preprocess/src/landslide_precip.py /scratch/summit/elcu4811/landslide.hydromet.data/precipitation/IMERGE/2017/3B-HHR-E.MS.MRG.3IMERG.2017\*-S\*.nc4 /scratch/summit/elcu4811/landslide.hydromet.data/landslide/landslides.verified.csv /projects/elcu4811/landslide.hydromet/landslide.hydromet.out/imerge/imerge.2017.csv lon lat precipitationCal time lon lat netcdf4 "EPSG:4326" info'
